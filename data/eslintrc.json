{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "brace-style": [
      2,
      "1tbs"
    ],
    "camelcase": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "complexity": 3,
    "computed-property-spacing": [
      2,
      "never"
    ],
    "consistent-this": [
      2,
      "that"
    ],
    "default-case": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "func-names": 2,
    "guard-for-in": 2,
    "handle-callback-err": [
      2,
      "^(err|error|[a-z]+Error|reason)$"
    ],
    "indent": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "max-depth": 3,
    "max-len": 100,
    "max-params": 3,
    "max-statements": 10,
    "new-cap": [
      2,
      {
        "capIsNew": true,
        "capIsNewExceptions": [
        ],
        "newIsCap": true,
        "newIsCapExceptions": [
        ]
      }
    ],
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-array-constructor": 2,
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-constant-condition": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inline-comments": 2,
    "no-inner-declarations": [
      2,
      "both"
    ],
    "no-mixed-spaces-and-tabs": [
      2,
      "smart-tabs"
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1
      }
    ],
    "no-native-reassign": [
      2,
      {
        "excpetions": []
      }
    ],
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-path-concat": 2,
    "no-proto": 2,
    "no-reserved-keys": 0,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-sparse-arrays": 2,
    "no-sync": 2,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": false
      }
    ],
    "no-undef": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-var": 2,
    "no-with": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "object-shorthand": [
      2,
      "always"
    ],
    "one-var": [
      2,
      "never"
    ],
    "operator-assignment": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-after-keywords": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": [
      2,
      {
        "int32Hint": true
      }
    ],
    "space-return-throw-case": 2,
    "valid-jsdoc": [
      2,
      {
      }
    ],
    "valid-typeof": 2
  }
}
